[user]
	email = joe.maples@valtech.com
	name = Joe Maples
	signingkey = 0FBB331F
[color]
	ui = auto
	ui = true
[alias]
	cp  = cherry-pick
	cpc = cherry-pick --continue
	cpa = cherry-pick --abort
	cpq = cherry-pick --quit
	hr  = reset --hard
[core]
	editor = nvim
	autocrlf = input
[gpg]
	program = gpg
[review "review.carbonrom.org"]
	username = frap129
	email = joe@maples.dev
[push]
	gpgSign = if-asked
[safe]
	directory = /opt/flutter
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
